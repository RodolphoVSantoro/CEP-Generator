{
  "name": "Cep/CPF generator",
  "description": "Gera CEPs e CPFs aleat√≥rios para testes",
  "version": "1.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup/cep.html"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/cep.js",
        "scripts/cpf.js"
      ],
      "matches": [
        "http://127.0.0.1/*"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "ceps/cep.bin"
      ],
      "matches": [
        "http://127.0.0.1/*"
      ]
    }
  ],
  "icons": { "16": "images/icon.png", "48": "images/icon.png", "128": "images/icon.png" }
}